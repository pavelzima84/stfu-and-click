{
  "parser"  : "babel-eslint",
  "extends" : ["airbnb"],
  "rules": {
  	"semi": 0,
  	"comma-dangle": 0,
  	"prefer-const": 0,
  	"no-use-before-define": 0,
  	"no-console": 0,
  	"one-var": 0,
    "default-case": 0,
    "react/prefer-stateless-function": 0,
    "react/wrap-multilines": 0,
    "spaced-comment": 0,
    "no-unused-vars": 0,
    "react/sort-comp": 0
  }
}
